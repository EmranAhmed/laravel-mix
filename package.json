  },
  "homepage": "https://github.com/EmranAhmed/wp-mix#readme",
  "keywords": [
    "laravel",
    "wordpress",
    "webpack",
    "laravel elixir",
    "laravel mix",
    "wp mix"
  ],
  "author": "Jeffrey Way",
  "license": "MIT",
